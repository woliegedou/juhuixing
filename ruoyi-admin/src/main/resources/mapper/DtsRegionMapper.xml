<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ruoyi.program.mapper.DtsRegionMapper">
    <resultMap id="BaseResultMap" type="com.ruoyi.program.entity.DtsRegion">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="pid" property="pid" jdbcType="INTEGER"/>
        <result column="name" property="name" jdbcType="VARCHAR"/>
        <result column="type" property="type" jdbcType="INTEGER"/>
        <result column="code" property="code" jdbcType="INTEGER"/>
        <result column="created_by" property="createdBy" jdbcType="VARCHAR"/>
        <result column="created_time" property="createdTime" jdbcType="TIMESTAMP"/>
        <result column="updated_by" property="updatedBy" jdbcType="VARCHAR"/>
        <result column="updated_time" property="updatedTime" jdbcType="TIMESTAMP"/>
        <result column="is_delete" property="isDelete" jdbcType="INTEGER"/>
    </resultMap>
    <insert id="insertdtsRegion">
        INSERT INTO dts_region
        (pid, name, type, code, created_by, created_time, updated_by, updated_time, is_delete)
        values (#{pid}, #{name}, #{type}, #{code}, #{createdBy}, #{createdTime}, #{updatedBy}, #{updatedTime},
                #{isDelete})
    </insert>
    <!--通过主键修改数据-->
    <update id="updatedtsRegion">
        update dts_region
        <set>
            <if test="pid != null">
                pid = #{pid},
            </if>
            <if test="name != null and name != ''">
                name = #{name},
            </if>
            <if test="type != null">
                type = #{type},
            </if>
            <if test="code != null">
                code = #{code},
            </if>
            <if test="createdBy != null and createdBy != ''">
                created_by = #{createdBy},
            </if>
            <if test="createdTime != null">
                created_time = #{createdTime},
            </if>
            <if test="updatedBy != null and updatedBy != ''">
                updated_by = #{updatedBy},
            </if>
            <if test="updatedTime != null">
                updated_time = #{updatedTime},
            </if>
            <if test="isDelete != null">
                is_delete = #{isDelete},
            </if>
        </set>
        where id = #{id}
    </update>
    <delete id="deletedtsRegion">
        DELETE
        FROM dts_region
        WHERE id = #{id}
    </delete>
    <select id="queryAllByLimit" resultMap="BaseResultMap">

        SELECT
        id,
        pid,
        name,
        type,
        code,
        created_by,
        created_time,
        updated_by,
        updated_time,
        is_delete
        FROM
        dts_region
        <where>
            <if test="id != null">
                AND id = #{id}
            </if>
            <if test="pid != null">
                AND pid = #{pid}
            </if>
            <if test="name != null and name != ''">
                AND name = #{name}
            </if>
            <if test="type != null">
                AND type = #{type}
            </if>
            <if test="code != null">
                AND code = #{code}
            </if>
            <if test="createdBy != null and createdBy != ''">
                AND created_by = #{createdBy}
            </if>
            <if test="createdTime != null">
                AND created_time = #{createdTime}
            </if>
            <if test="updatedBy != null and updatedBy != ''">
                AND updated_by = #{updatedBy}
            </if>
            <if test="updatedTime != null">
                AND updated_time = #{updatedTime}
            </if>
            <if test="isDelete != null">
                AND is_delete = #{isDelete}
            </if>
        </where>
    </select>
    <select id="DtsRegionid" resultMap="BaseResultMap">
        SELECT id,
               pid,
               name,
               type,
               code,
               created_by,
               created_time,
               updated_by,
               updated_time,
               is_delete
        FROM dts_region
        WHERE id = #{id}
    </select>
    <select id="dtsRegionids" resultMap="BaseResultMap">
        SELECT id,
        pid,
        name,
        type,
        code,
        created_by,
        created_time,
        updated_by,
        updated_time,
        is_delete
        FROM dts_region
        <where>
            <if test="id != null">
                AND id = #{id}
            </if>
            <if test="pid != null">
                AND pid = #{pid}
            </if>
            <if test="name != null and name != ''">
                AND name LIKE CONCAT('%', #{name}, '%')
            </if>
            <if test="type != null">
                AND type = #{type}
            </if>
            <if test="code != null">
                AND code = #{code}
            </if>
            <if test="createdBy != null and createdBy != ''">
                AND created_by = #{createdBy}
            </if>
            <if test="createdTime != null">
                AND created_time = #{createdTime}
            </if>
            <if test="updatedBy != null and updatedBy != ''">
                AND updated_by = #{updatedBy}
            </if>
            <if test="updatedTime != null">
                AND updated_time = #{updatedTime}
            </if>
            <if test="isDelete != null">
                AND is_delete = #{isDelete}
            </if>
        </where>
    </select>
    <select id="searchMajorByName" resultType="com.ruoyi.program.entity.DtsRegion">
        SELECT id,
               pid,
               name,
               type,
               code,
               created_by,
               created_time,
               updated_by,
               updated_time,
               is_delete
        FROM dts_region
        WHERE name LIKE '%${name}%'
    </select>


</mapper>